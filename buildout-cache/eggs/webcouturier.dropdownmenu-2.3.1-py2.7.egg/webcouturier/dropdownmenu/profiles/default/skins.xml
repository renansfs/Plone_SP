<?xml version="1.0"?>
<object name="portal_skins">

 <object name="dropdownmenu" meta_type="Filesystem Directory View"
    directory="webcouturier.dropdownmenu:skins/dropdownmenu"/>
 <object name="dropdownmenu_sunburst" meta_type="Filesystem Directory View"
    directory="webcouturier.dropdownmenu:skins/dropdownmenu_sunburst"/>

 <skin-path name="*">
  <layer name="dropdownmenu"/>
 </skin-path>

 <skin-path name="Sunburst Theme">
  <layer name="dropdownmenu_sunburst"
         insert-before="dropdownmenu" />
 </skin-path>

</object>
